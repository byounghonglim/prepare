<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="prepare.byounghong.prepare">

    <uses-permission android:name="android.permission.INTERNET"/>


    <meta-data android:name="com.leopold.android.riders.network.GlideConfiguration" android:value="GlideModule" />
    <meta-data android:name="com.bumptech.glide.integration.okhttp3.OkHttpGlideModule"  />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.hello.HelloActivity"/>
        <activity android:name=".view.count.CountActivity"/>
        <activity android:name=".view.progress.ProgressActivity"/>

    </application>

</manifest>